<ul class="nav">
  <li><%= link_to "All Pets", "/pets" %></li>
  <li><%= link_to "All Shelters", "/shelters" %></li>
</ul>
<%= form_tag "/shelters" do %>
<p><%= label_tag :name, "Name:" %> <%= text_field_tag :name %></p>
<p><%= label_tag :address, "Address:" %> <%= text_field_tag :address %></p>
<p><%= label_tag :city, "City:"%> <%= text_field_tag :city %></p>
<p><%= label_tag :state, "State:"%> <%= text_field_tag :state %></p>
<p><%= label_tag :zip, "Zip:" %> <%= text_field_tag :zip %></p>
<%= submit_tag 'Create Shelter' %>
<% end %>
