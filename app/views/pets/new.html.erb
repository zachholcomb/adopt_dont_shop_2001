<%= form_tag "/shelters/#{@shelter.id}/pets" do %>
<p><%= label_tag :name, "Name:" %> <%= text_field_tag :name %></p>
<p><%= label_tag :age, "Age:" %> <%= text_field_tag :age %></p>
<p><%= label_tag :sex, "Sex:"%> <%= text_field_tag :sex %></p>
<p><%= label_tag :description, "Description:"%> <%= text_field_tag :description %></p>
<p><%= label_tag :image, "Image:" %> <%= text_field_tag :image %></p>
<%= submit_tag 'Create Pet' %>
<% end %>
